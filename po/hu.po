#: src/gnome-nibbles.vala:84 src/gnome-nibbles.vala:90
msgid "NUMBER"
msgstr "SZÁM"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:72
msgid "Turn off the sound"
msgstr "Hang kikapcsolása"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:75
msgid "Set number of nibbles (4-6)"
msgstr "A kukacok számának beállítása (4-6)"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:81
msgid "Set number of players (1-4)"
msgstr "A játékosok számának beállítása (1-4)"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:87
msgid "Set worms speed (4-1, 4 for slow)"
msgstr "A kukacok sebességének beállítása (4-1, a 4 a lassú)"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:93
msgid "Start playing"
msgstr "Játék indítása"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:96
msgid "Turn on the sound"
msgstr "Hang bekapcsolása"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:99
msgid "Show release version"
msgstr "Kiadási verziószám megjelenítése"

#. Translators: command-line error message, displayed for an invalid start level request; see 'gnome-nibbles -l 0'
#: src/gnome-nibbles.vala:132
msgid "Start level should only be between 1 and 26."
msgstr "A kezdőszintnek 1 és 26 közé kell esnie."

#. Translators: command-line error message, displayed for an invalid number of nibbles; see 'gnome-nibbles -n 1'
#: src/gnome-nibbles.vala:138
msgid "There could only be between 4 and 6 nibbles."
msgstr "A kukacok számának 4 és 6 közé kell esnie."

#. Translators: command-line error message, displayed for an invalid number of players; see 'gnome-nibbles -p 5'
#: src/gnome-nibbles.vala:144
msgid "There could only be between 1 and 4 players."
msgstr "A játékosok számának 1 és 4 közé kell esnie."

#. Translators: command-line error message, displayed for an invalid given worms speed; see 'gnome-nibbles -s 5'
#: src/gnome-nibbles.vala:150
msgid "Speed should be between 4 (slow) and 1 (fast)."
msgstr "A sebességnek 4 (lassú) és 1 (gyors) közé kell esnie."

#. Translators: command-line error message, displayed for an invalid combination of options; see 'gnome-nibbles -d -e'
#: src/gnome-nibbles.vala:159
msgid ""
"Options --disable-fakes (-d) and --enable-fakes (-e) are mutually exclusive."
msgstr ""
"A --disable-fakes (-d) és az --enable-fakes (-e) kapcsolók kölcsönösen "
"kizárják egymást."

