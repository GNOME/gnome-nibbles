msgid "NUMBER"
msgstr "NOMBRO"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:74
msgid "Turn off the sound"
msgstr "Malŝalti la sonon"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:77
msgid "Set number of nibbles (4-6)"
msgstr "Agordi nombron da mordetoj (4-6)"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:83
msgid "Set number of players (1-4)"
msgstr "Agordi nombron da ludantoj (1-4)"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:89
msgid "Set worms speed (4-1, 4 for slow)"
msgstr "Agordi verman rapidecon (4-1, 4 malrapidas)"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:95
msgid "Set three dimensional view"
msgstr "Agordi tri-dimensian aspekton"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:98
msgid "Set two dimensional view"
msgstr "Agordi du-dimensian aspekton"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:101
msgid "Start playing"
msgstr "Komenci ludi"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:104
msgid "Turn on the sound"
msgstr "Ŝalti la sonon"

#. Translators: command-line option description, see 'gnome-nibbles --help'
#: src/gnome-nibbles.vala:107
msgid "Show release version"
msgstr "Montri eldonversion"

#. Translators: command-line error message, displayed for an invalid start level request; see 'gnome-nibbles -l 0'
#: src/gnome-nibbles.vala:148
msgid "Start level should only be between 1 and 26."
msgstr "Komenca nivelo nur povas esti inter 1 kaj 26."

#. Translators: command-line error message, displayed for an invalid number of nibbles; see 'gnome-nibbles -n 1'
#: src/gnome-nibbles.vala:154
msgid "There could only be between 4 and 6 nibbles."
msgstr "Nur povas esti inter 4 kaj 6 mordetoj."

#. Translators: command-line error message, displayed for an invalid number of players; see 'gnome-nibbles -p 5'
#: src/gnome-nibbles.vala:160
msgid "There could only be between 1 and 4 players."
msgstr "Nur povas esti inter 1 kaj 4 ludantoj."

#. Translators: command-line error message, displayed for an invalid given worms speed; see 'gnome-nibbles -s 5'
#: src/gnome-nibbles.vala:166
msgid "Speed should be between 4 (slow) and 1 (fast)."
msgstr "Rapideco devas esti inter 4 (malrapide) kaj 1 (rapide)."

#. Translators: command-line error message, displayed for an invalid combination of options; see 'gnome-nibbles -d -e'
#: src/gnome-nibbles.vala:175
msgid ""
"Options --disable-fakes (-d) and --enable-fakes (-e) are mutually exclusive."
msgstr ""
"Opcioj --disable-fakes (-d) kaj --enable-fakes (-e) estas reciproke "
"ekskludaj."

#. Translators: command-line error message, displayed for an invalid combination of options; see 'gnome-nibbles -3 -2'
#: src/gnome-nibbles.vala:187
msgid "Options --3D (-3) and --2D (-2) are mutually exclusive."
msgstr "Opcioj --3D (-3) kaj --2D (-2) estas reciproke ekskludaj."

